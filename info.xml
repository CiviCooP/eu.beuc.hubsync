<?xml version="1.0"?>
<extension key="eu.beuc.hubsync" type="module">
  <file>hubsync</file>
  <name>BEUC HUB Sync</name>
  <description>Synchronizes data from HUB with CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-08-07</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Check the settings civicrm/beuchubsync/settings, and enable the scheduled job.</comments>
  <civix>
    <namespace>CRM/Hubsync</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Hubsync_Upgrader</upgrader>
</extension>
